<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="215.0" prefWidth="232.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demokitudjahanyadik.AdventureController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label text="Lost in The Wild" textAlignment="CENTER">
      <font>
         <Font size="24.0" />
      </font></Label>
    <Button fx:id="startbutton" onAction="#onStartButtonClick" text="Start" />
   <Button fx:id="exitbutton" mnemonicParsing="false" onAction="#onExitButtonClick" prefHeight="29.0" prefWidth="45.0" text="Exit" />
</VBox>
